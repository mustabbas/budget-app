
<div class ="p-2 text-end"><%= link_to 'Add Category', new_category_path %></div>
<% @category.each do |category| %>
  <div class="row border m-5 p-5">
     <div class="col-4">
        <h1><%= link_to category.name, transaction_show_path(category.id)  %></h1>
     </div>
     <div class="col-4">
            <h1><%= category.icon %></h1>
     </div>
  </div>
  <% end %>